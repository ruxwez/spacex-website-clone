---
import Footer from "../components/footer.astro";
import Navbar from "../components/navbar.astro";
import LaunchsSection from "../components/section/home/launchsSection.astro";
import Layout from "../layouts/Layout.astro";
---

<Layout title="SpaceX Clone Home">
  <Navbar />

  <LaunchsSection
    content={{
      isComing: true,
      showSubtitle: true,
      title: "Starship's Sixth Flight Test",
      href: "https://www.spacex.com/launches/mission/?missionId=starship-flight-6",
    }}
    type="header"
    isVideo
    src="/videos/header_background.mp4"
  />

  <LaunchsSection
    content={{
      isComing: true,
      showSubtitle: true,
      title: "Starlink Mission",
      href: "https://www.spacex.com/launches/mission/?missionId=sl-9-11",
    }}
    type="section"
    src="/images/Launch_Page_Starlink1_vertical_18_DESKTOP_c051df793a.jpg"
  />

  <LaunchsSection
    content={{
      showSubtitle: true,

      title: "Koreasat-6A Mission",
      href: "https://www.spacex.com/launches/mission/?missionId=koreasat-6a",
    }}
    type="section"
    src="/images/Koreasat6_A_UCS_DSC_8729_mobile_c2244ce741.jpg"
  />

  <LaunchsSection
    content={{
      showSubtitle: true,
      title: "Starlink Mission",
      href: "https://www.spacex.com/launches/mission/?missionId=sl-6-69",
    }}
    type="section"
    src="/images/starlink_6_69_desktop_5f9e895a09.jpg"
  />

  <LaunchsSection
    content={{
      title: "ADVANCING HUMAN SPACEFLIGHT",
      href: "https://www.spacex.com/updates/#eva-suit",
      linkTitle: "Learn More",
    }}
    type="section"
    src="/images/Eva_Suit_Desktop_alternate_1cf9bae18e.jpg"
  />

  <LaunchsSection
    content={{

      title: "TO MAKE LIFE MULTIPLANETARY",
      href: "https://www.spacex.com/updates/#eva-suit",
      linkTitle: "Learn More",
    }}
    type="section"
    src="/images/make_life_multiplanetary_desktop_3fa7cff73c.jpg"
  />
  <Footer/>
</Layout>
